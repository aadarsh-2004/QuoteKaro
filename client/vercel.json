{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.quotekaro.in/$1",
      "statusCode": 308,
      "permanent" : true, 
      "has": [
        {
          "type": "host",
          "value": "quotekaro.in"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
